<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="never" page-range-format="expanded">
  <info>
    <title>Test</title>
    <id>f48846b9-384c-40cd-93b5-921bd01504e8</id>
    <updated>2025-02-12T22:00:00+00:00</updated>
  </info>
  <macro name="author-bib">
    <names variable="author">
      <substitute>
        <names variable="editor"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="date-bib">
    <group delimiter=" " prefix="(" suffix=")">
      <choose>
        <if variable="issued">
          <date variable="issued" date-parts="year" form="numeric"/>
          <text variable="year-suffix"/>
        </if>
        <else>
          <text term="no date" form="short"/>
          <text variable="year-suffix" prefix="-"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if variable="title">
        <text variable="title"/>
      </if>
      <else>
        <text value="No title"/>
      </else>
    </choose>
  </macro>
  <macro name="author-intext">
    <names variable="author" delimiter=", ">
      <name form="short" and="symbol" delimiter=", " initialize-with=". "/>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI" match="any">
        <text variable="DOI" prefix="https://doi.org/"/>
      </if>
      <else-if variable="URL">
        <group delimiter=" ">
          <text variable="URL"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="date-intext">
    <text term="no date" form="short"/>
    <text variable="year-suffix" prefix="-"/>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" collapse="year" givenname-disambiguation-rule="primary-name-with-initials">
    <sort>
      <key macro="author-bib"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", ">
        <text macro="author-intext"/>
        <text macro="date-intext"/>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true" et-al-min="21" et-al-use-first="19" et-al-use-last="true" entry-spacing="0" line-spacing="2">
    <sort>
      <key macro="author-bib"/>
      <key macro="title"/>
    </sort>
    <layout>
      <group delimiter=". " suffix=".">
        <text macro="author-bib"/>
        <text macro="date-bib"/>
        <text macro="title"/>
        <text macro="access"/>
      </group>
    </layout>
  </bibliography>
</style>
